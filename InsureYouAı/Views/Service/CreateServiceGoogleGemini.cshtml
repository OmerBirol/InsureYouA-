@{
    ViewData["Title"] = "CreateServiceGoogleGemini";
    Layout = "~/Views/AdminLayout/Index.cshtml";
    var services = ViewBag.services as List<string>;
}

<div class="container mt-5">
    <h2>Gemini ile Oluşturulan Hizmetler</h2>
    <ul class="list-group mt-3">
        @if (services != null && services.Count > 0)
        {
            foreach (var item in services)
            {
                <li class="list-group-item">@item</li>
            }
        }
        else
        {
            <li class="list-group-item text-danger">Gemini'den veri alınamadı.</li>
        }
    </ul>
</div>
