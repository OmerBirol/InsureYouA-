@using InsureYouAıNew.Entities
@model Service

@{
    ViewData["Title"] = "UpdateService";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}






<div class="card">
    <div class="card-body">
        <div class="d-flex align-items-center">
            <h5 class="mb-0">Yeni hizmet girişi</h5>

        </div>
        <form method="post">
			<input type="hidden" asp-for="ServiceId" />
            <label>Başlık</label>
            <input type="text" asp-for="Title" class="form-control" />
            <br />

            <label>Açıklama</label>
            <input type="text" asp-for="Description" class="form-control" />
            <br />

            <label>Görsel URL </label>
            <input type="text" asp-for="ImageUrl" class="form-control" />
            <br />

            <label>İkon url </label>
            <input type="text" asp-for="IconUrl" class="form-control" />
            <br />


            <button class="btn btn-success">Hizmeti kaydet</button>
        </form>
    </div>
</div>




