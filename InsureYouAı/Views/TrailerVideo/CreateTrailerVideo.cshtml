@using InsureYouAıNew.Entities
@model TrailerVideo
@{
    ViewData["Title"] = "CreateTrailerVideo";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}










<div class="card">
    <div class="card-body">
        <div class="d-flex align-items-center">
            <h5 class="mb-0">Yeni referans yorum  girişi</h5>

        </div>
        <form method="post">
          

            <label>Video Başlığı </label>
            <input type="text" asp-for="Title" class="form-control" />
            <br />

            <label>Kapak fotosu </label>
            <input type="text" asp-for="CoverImageUrl" class="form-control" />
            <br />

            <label>Video youtube embed Url </label>
            <input type="text" asp-for="VideoUrl" class="form-control" />
            <br />
            <button class="btn btn-primary">Referansı kaydet</button>
        </form>
    </div>
</div>







