@using InsureYouAıNew.Entities
@model PricingPlan
@{
    ViewData["Title"] = "CreatePricingPlan";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}



<div class="card">
    <div class="card-body">
        <div class="d-flex align-items-center">
            <h5 class="mb-0">Yeni fiyat planı  girişi</h5>

        </div>
        <form method="post">
            <label>Başlık</label>
            <input type="text "asp-for="Title" class="form-control" />
            <br />

            <label>Fiyat</label>
            <input type="text asp-for="Price" class="form-control" />
            <br />

            
            <button class="btn btn-primary">Planı kaydet</button>
        </form>
    </div>
</div>


